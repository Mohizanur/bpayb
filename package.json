{
  "name": "birrpay-clone-bot",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "prestart": "node scripts/verify-env.js",
    "start": "cross-env NODE_OPTIONS='--no-warnings=ExperimentalWarning --unhandled-rejections=strict' NODE_ENV=production node -r ./scripts/suppress-debug.cjs src/index.js",
    "dev": "cross-env NODE_OPTIONS='--no-warnings=ExperimentalWarning' NODE_ENV=development node --watch src/index.js",
    "test": "node --test tests/*.test.js",
    "test:watch": "node --test --watch tests/*.test.js",
    "test:coverage": "c8 node --test tests/*.test.js",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "build": "npm install --production=false && npm dedupe && npm cache clean --force",
    "serve": "cross-env NODE_OPTIONS='--no-warnings=ExperimentalWarning' NODE_ENV=development node src/index.js",
    "check:services": "node scripts/checkServices.js",
    "prod": "cross-env NODE_OPTIONS='--no-warnings=ExperimentalWarning' NODE_ENV=production node -r ./scripts/suppress-debug.cjs src/index.js"
  },
  "dependencies": {
    "@fastify/send": "^1.2.0",
    "@fastify/static": "^7.0.0",
    "@google-cloud/firestore": "^7.8.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fastify": "^4.23.0",
    "fastify-plugin": "^4.5.0",
    "firebase-admin": "^12.0.0",
    "gaxios": "^6.0.0",
    "glob": "^10.3.10",
    "google-auth-library": "^9.1.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "pump": "^3.0.2",
    "send": "^0.19.0",
    "telegraf": "^4.12.2",
    "telegraf-i18n": "^6.5.0",
    "uuid": "^9.0.1"
  },
  "overrides": {
    "debug": "^4.3.4"
  },
  "engines": {
    "node": ">=18.20.2 <=20.x"
  },
  "packageManager": "npm@10.2.0",
  "devDependencies": {
    "c8": "^8.0.1",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "telegram-bot",
    "subscription-management",
    "firebase",
    "birrpay",
    "ethiopia"
  ],
  "author": "BirrPay Team",
  "license": "MIT",
  "description": "BirrPay - Ethiopia's Premier Subscription Management Platform"
}
